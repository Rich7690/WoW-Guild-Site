{
  "name": "wow-guild-static",
  "version": "1.0.0",
  "description": "",
  "private": true,
  "directories": {
    "lib": "lib"
  },
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "rm -rf dist && webpack --config webpack.prod.config.js",
    "gulp": "gulp clean && gulp",
    "webpack": "webpack",
    "start": "npm run serve",
    "start:dev": "webpack-dev-server --config webpack.dev.config.js",
    "publish": "aws s3 sync ./dist s3://wow-guild-site-production --exclude \"*.html\" --delete --cache-control \"max-age=31557600, public,immutable\" && npm run publish-html",
    "publish-html": "aws s3 cp ./dist/ s3://wow-guild-site-production --exclude \"*\" --include \"*.html\" --recursive --cache-control \"max-age=3600, public, must-revalidate\"",
    "build-publish": "npm run gulp && npm run publish"
  },
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.5.5",
    "babel-preset-es2015": "^6.24.1",
    "clean-webpack-plugin": "^3.0.0",
    "copy-webpack-plugin": "^5.0.4",
    "css-loader": "^3.0.0",
    "extract-loader": "^3.1.0",
    "file-loader": "^4.0.0",
    "gulp": "^4.0.2",
    "gulp-babel": "^8.0.0",
    "gulp-concat": "^2.6.1",
    "gulp-connect": "^5.7.0",
    "gulp-inject": "^5.0.4",
    "gulp-livereload": "^4.0.1",
    "gulp-minify": "^3.1.0",
    "gulp-rev": "^9.0.0",
    "gulp-sourcemaps": "^2.6.5",
    "gulp-uglify": "^3.0.2",
    "html-loader": "^0.5.5",
    "html-webpack-plugin": "^3.2.0",
    "http-proxy-middleware": "^0.19.1",
    "mini-css-extract-plugin": "^0.7.0",
    "rimraf": "^2.6.3",
    "style-loader": "^0.23.1",
    "webpack": "^4.35.3",
    "webpack-cli": "^3.3.5",
    "webpack-dev-server": "^3.7.2"
  },
  "dependencies": {
    "angular": "^1.7.8",
    "angular-http-etag": "^2.0.18",
    "angular-local-storage": "^0.7.1",
    "angular-promise-buttons": "^0.1.23",
    "angular-route": "^1.7.8",
    "local-storage": "^2.0.0"
  }
}
